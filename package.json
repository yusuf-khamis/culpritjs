{
  "name": "culpritjs",
  "description": "JavaScript validation inspired by Laravel validation",
  "version": "1.0.9",
  "scripts": {
    "pretest": "npm run standard",
    "test": "jest --coverage",
    "standard": "standard"
  },
  "main": "lib/index.js",
  "files": [
    "lib/",
    "index.d.ts"
  ],
  "dependencies": {
    "moment-timezone": "^0.5.23"
  },
  "license": "MIT",
  "keywords": [
    "validation",
    "validate",
    "browser",
    "browser-validation",
    "laravel",
    "laravel-validation",
    "schema",
    "culprit",
    "culpritjs"
  ],
  "standard": {
    "env": [
      "jest"
    ],
    "globals": [
      "window"
    ],
    "ignore": [
      "lib/types/array.js",
      "lib/types/date.js",
      "lib/types/number.js",
      "lib/types/string.js",
      "test/types/array.js",
      "test/types/date.js",
      "test/types/number.js",
      "test/types/string.js",
      "test/test.js",
      "test/validator.js"
    ]
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "standard": "^14.3.0"
  },
  "types": "./index.d.ts"
}
